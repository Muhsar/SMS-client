    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.4",
    "compression": "^1.7.4",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "jquery": "^3.5.1",
    "jwt-decode": "^3.0.0",
    "mime-types": "^2.1.27",
    "moment": "^2.29.0",
    "morgan": "^1.10.0",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-chartjs-2": "^2.10.0",
    "react-countup": "^4.3.3",
