    "socket.io-client": "^2.3.1",
    "styled-components": "^5.2.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "windows": "electron-builder --windows",
    "electron-build": "electron-builder --linux deb",
    "fullbuild":"yarn run windows && yarn run electron-build",
    "build": "yarn run react-build && yarn run electron-build",
    "desktop": "wait-on http://localhost:3000 && electron .",
    "start": "concurrently \"BROWSER=none yarn run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "wait-on": "^5.2.0"
  }
}
