    "react-moment": "^1.0.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-toastify": "^6.0.8",
    "react-transition-group": "^4.4.1",
    "reactstrap": "^8.6.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
